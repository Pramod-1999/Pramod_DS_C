//Write a C program to Accept elements into Array.

#include<stdio.h>
int main()
{
    int a[100],i,n;
    printf("Enter the number of elements in the Array : ");
    scanf("%d",&n);
    printf("\nEnter the %d elements of the Array : ",n);
    for(i=0;i<n;i++)
        scanf("%d",&a[i]);
    printf("\nThe %d elements of the Array are : ",n);
    for(i=0;i<n;i++)
        printf("\t%d",a[i]);
}
